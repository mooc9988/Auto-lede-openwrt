CONFIG_TARGET_ramips_mt7621=y
CONFIG_TARGET_ramips_mt7621_DEVICE_raisecom_msg1500-x-00=y
#CONFIG_TARGET_IMAGES_GZIP=y
#CONFIG_TARGET_MULTI_PROFILE=y
#CONFIG_TARGET_ROOTFS_INITRAMFS=y

