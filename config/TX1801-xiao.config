CONFIG_PACKAGE_kmod-ip6tables-extra=y
CONFIG_PACKAGE_iputils-traceroute6=y
CONFIG_PACKAGE_6in4=y
CONFIG_PACKAGE_6rd=y
CONFIG_PACKAGE_6to4=y

#
# USB与存储相关支持
